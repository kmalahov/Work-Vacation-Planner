@model Practice.Models.Worker
@{
    ViewBag.Title = "Добавление пользователя";
}
<form asp-action="create" asp-controller="home">
    <div class="form-group">
        <label asp-for="Name" class="control-label">Имя</label>
        <input type="text" asp-for="Name" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Surname" class="control-label">Возраст</label>
        <input type="number" asp-for="Surname" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Patronymic" class="control-label">Возраст</label>
        <input type="number" asp-for="Patronymic" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="ServiceNumber" class="control-label">Возраст</label>
        <input type="number" asp-for="ServiceNumber" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Post" class="control-label">Возраст</label>
        <input type="number" asp-for="Post" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Department" class="control-label">Возраст</label>
        <input type="number" asp-for="Department" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Email" class="control-label">Возраст</label>
        <input type="number" asp-for="Email" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Phone" class="control-label">Возраст</label>
        <input type="number" asp-for="Phone" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="DateHiring" class="control-label">Возраст</label>
        <input type="number" asp-for="DateHiring" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Gender" class="control-label">Возраст</label>
        <input type="number" asp-for="Gender" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Отправить" class="btn btn-default" />
    </div>
</form>
